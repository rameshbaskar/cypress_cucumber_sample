{
    "testFiles": "**/*.feature",
    "baseUrl": "http://localhost:3000",
    "chromeWebSecurity": false,
    "modifyObstructiveCode": true,
    "viewportWidth": 1200,
    "viewportHeight": 1300,
    "video": false,
    "reporter": "mochawesome",
    "reporterOptions": {
        "reportDir": "cypress/results/json",
        "overwrite": false,
        "html": false,
        "json": true
    },
    "defaultCommandTimeout": 90000,
    "requestTimeout": 90000,
    "responseTimeout": 90000
}
